{
  "name": "vue-validate-ylli",
  "version": "1.0.0",
  "description": "Simple Vue.js form validation library",
  "main": "dist/vue-validate-ylli.common.js",
  "author": "Ylli Gorce",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ylligorce/vue-validate-ylli.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve src/test/main.js",
    "build": "vue-cli-service build --target lib --name vue-validate-ylli src/index.js",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli": "^4.2.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "core-js": "^2.6.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
